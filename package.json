{
  "name": "react-starter-kit",
  "version": "0.1.0",
  "private": true,
  "description": "A progressive React starter kit based on the Atomic Design methodology",
  "author": "Masood Ahmed <masood.ahmed09@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/masood09/react-starter-kit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masood09/react-starter-kit.git"
  },
  "bugs": {
    "url": "https://github.com/masood09/react-starter-kit/issues"
  },
  "keywords": [
    "react",
    "starterkit",
    "JSX",
    "ES6",
    "webpack",
    "eslint",
    "stylelint",
    "babel",
    "react-storybook"
  ],
  "engines": {
    "node": ">=6.2.1",
    "npm": ">=3.9.3"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 11"
  ],
  "pre-commit": [
    "lint"
  ],
  "scripts": {
    "build:clean": "del \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "build": "cross-env NODE_ENV=production webpack",
    "lint:js": "eslint --ext .js --ext .jsx server.js webpack.config.js src .storybook",
    "lint": "npm run lint:js",
    "prebuild": "npm run build:clean && npm run build:copy",
    "start": "node server",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "classnames": "^2.2.5",
    "compression-webpack-plugin": "^0.3.2",
    "copyfiles": "^1.2.0",
    "cross-env": "^3.2.4",
    "del-cli": "^0.2.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-no-unused-vars-rest": "^1.0.4",
    "eslint-plugin-react": "^6.10.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "pre-commit": "^1.2.2",
    "react-hot-loader": "next",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  }
}
